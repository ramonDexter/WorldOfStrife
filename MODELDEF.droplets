Model BloodFog      // Name of actor in DECORATE
{
   Path "models/bloodspot/"    // Path to model in PK3
   Model 0 "bloodspot.md3"    // Model index, model file
   Skin 0 "bloodfog.png"     // Model index, texture (can be in any format supported by GZDoom)
   Scale 15 15 15            // Scale values
   ZOffset 0.11
   FrameIndex BFOG A 0 0         // The sprite lump, sprite frame, model index, frame number
}

